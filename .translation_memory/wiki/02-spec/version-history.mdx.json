      }
    },
    {
      "segment_id": "c504c207",
      "source_content": "The following app versions correspond to the `version` key in projects:",
      "source_content_hash": "ba9d33b6922ca100da8883fe0a151ad4bec9b5d02a5452f9111d3db021e11814",
      "node_type": "paragraph",
      "translatable": true,
      "translations": {
        "ja": "以下のアプリバージョンはプロジェクト内の`version`キーに対応しています："
      }
    },
    {
      "segment_id": "c6cc90a0",
      "source_content": "### &lt;&equals;24",
      "source_content_hash": "8eac32d421decc0c0aa37555820adcb964835d8324d59921edeaa7a24198d6e5",
      "node_type": "heading",
      "translatable": true,
