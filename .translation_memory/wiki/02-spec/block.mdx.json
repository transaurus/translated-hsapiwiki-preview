    {
      "segment_id": "a9b642df",
      "source_content": "import {PropertyData} from '/src/components/PropertyData.tsx';\nimport Undocumented from '/src/components/Undocumented.tsx';\nimport Note from '/src/components/Note.tsx';",
      "source_content_hash": "8ae9bbbb2e33e1a10e141b1f00c70b16f30026b385f611abda0af54dc96c382a",
      "node_type": "mdxjsEsm",
